{
    "language": "python",
    "display_name": "Spark H2o Operator (Python)",
    "metadata": {
      "process_proxy": {
        "class_name": "enterprise_gateway.services.processproxies.spark_operator.SparkOperatorProcessProxy",
        "config": {
          "image_name": "spark:3.5.1-scala2.12-java11-python3-ubuntu",
          "executor_image_name": "spark:3.5.1-scala2.12-java11-python3-ubuntu"
        }
      }
    },
    "argv": [
      "python",
      "/usr/local/share/jupyter/kernels/spark_python_operator/scripts/launch_custom_resource.py",
      "--RemoteProcessProxy.kernel-id",
      "{kernel_id}",
      "--RemoteProcessProxy.port-range",
      "{port_range}",
      "--RemoteProcessProxy.response-address",
      "{response_address}",
      "--RemoteProcessProxy.public-key",
      "{public_key}"
    ]
}